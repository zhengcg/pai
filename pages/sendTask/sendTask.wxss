.tabNav{
  display: flex;
  position: fixed;
  background: #FFFFFF;
  box-shadow: 0 0 24px 0 rgba(0,0,0,0.12);
  height: 112rpx;
  width: 750rpx;
  top: 0;
  left: 0;
  z-index: 111;
}
.tabNav navigator{
  width: 50%;
  text-align: center;
  height: 106rpx;
  line-height: 106rpx;
  font-size: 32rpx;
  color: #666666;
}
.tabNav navigator.select{
  color: #22A7F0;
  border-bottom: 6rpx solid #22A7F0
}
.status{
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  color: #666;
  font-size: 28rpx;
  background: #fff;
}
.inputBox{
  width: 686rpx;
  background: #F7F7F7;
  
}
.inputBox .box{
  height: 120rpx;
  font-size: 32rpx;
  color: #666666;
  line-height: 120rpx;
   display: flex;
   padding: 0 32rpx 0 40rpx;
   align-items:center;
}
.inputBox .box view{
  width: 464rpx;
  height: 72rpx;
  line-height: 72rpx;
  background: #fff;
  margin-left: 12rpx;
}
.inputBox .box view input{
  width: 380rpx;
  height: 72rpx;
  line-height: 72rpx;
  box-sizing: border-box;
  padding-left: 32rpx;
  color: #3C3C4B;
  float: left
}
.inputBox .box view text{
  font-size: 28rpx;
  color: #22A7F0;
}
.formBox{
  background: #fff;
  width: 686rpx;
  margin: 28rpx auto;
  background: #FFFFFF;
  box-shadow: 0 0 20rpx 0 rgba(50,51,52,0.12);
  border-radius: 8rpx;

}
.radioBox{
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
  align-items:center;
  padding: 28rpx;
}
.radioBox view{
  width: 200rpx;
  height: 140rpx;
  background: #F6F8FC;
  border: 0 solid #E5E5E5;
  border-radius: 32rpx;
  margin-bottom: 28rpx;
  font-size: 32rpx;
  color: #262628;
  text-align: center;
  line-height: 70rpx;
}
.radioBox view text{
  display: block;
}
.radioBox view.select{
  background: #22A7F0;
  color: #fff;
}
.selfBox{
  color: #222222;
  font-size: 32rpx;
  padding: 0 40rpx;
  display: flex;
  align-items:center;
  margin-bottom: 30rpx;
}
.selfBox input{
  border-bottom: 2rpx solid #aaa;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 24rpx;
  color: #AAAAAA;
  width: 380rpx;

}
.boxs{
  padding:30rpx 40rpx;
   color: #222222;
  font-size: 32rpx;
  line-height: 40rpx;
}
.redBox{
  font-size: 36rpx;
  color: #F45B5B;
  text-align: center;
  padding: 40rpx 0;
}
.madeCode{
  margin: 0 40rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-image: linear-gradient(-135deg, #22A7F0 0%, #3A75C2 100%);
box-shadow: 0 4rpx 12rpx 0 rgba(19,69,134,0.28);
border-radius: 44rpx;
font-size: 30rpx;
color: #FFFFFF;
}