/* pages/goAuthentication/goAuthentication.wxss */
.mainBox{
  background: #FFFFFF;
  box-shadow: 0 0 2rpx 0 rgba(50,51,52,0.12);
  border-radius: 8rpx;
}
.inputBox{
  background: #F7F7F7;
  height: 190rpx;
  border-bottom: 2rpx solid #F0F0F0;
  padding: 30rpx 40rpx 24rpx 40rpx;
  box-sizing: border-box;
}
.inputBox>text{
  font-size: 28rpx;
  color: #666666;
}
.inputBox>view{
  background: #fff;
  height: 72rpx;
  line-height: 72rpx;
  padding: 0 22rpx;
  font-size: 28rpx;
  color: #AAAAAA;
  margin-top: 14rpx;
  position: relative
}
.inputBox input{
  height: 72rpx;
}
.inputBox>view text{
  position: absolute;
  display: block;
  width: 100rpx;
  text-align: center;
  height: 72rpx;
  line-height: 72rpx;
  color: #22A7F0;
  right: 0;top: 0;

}
.imgBox{
  padding: 40rpx;
}
.img{
  height: 440rpx;
  width: 558rpx;
  margin: 0 auto;
  background:  #F7F7F7 url("data:image/png;base64,") no-repeat center;
  background-size: 109rpx auto;
  border: 2rpx solid #EAEDF3;
}
.imgBox button{
  margin-top: 40rpx;
  background-image: linear-gradient(-135deg, #22A7F0 0%, #3A75C2 100%);
box-shadow: 0 4rpx 12rpx 0 rgba(19,69,134,0.28);
height: 88rpx;
border-radius: 44rpx;
color: #fff;
font-size: 30rpx;
line-height: 88rpx;
}